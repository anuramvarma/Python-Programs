   "source": [
    "def rfact(n):\n",
    "   if n==0:\n",
    "      return 1\n",
    "   else:   \n",
    "      return n*rfact(n-1)\n",
    "x=int(input(\"Enter a Number to check it's Factorial : \"))\n",
    "print(\"Factorial of\",x,\"is:\",rfact(x))  "
   ]
  }
